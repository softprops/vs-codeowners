{
	"name": "vs-codeowners",
	"displayName": "vs-codeowners",
	"description": "Make use of GitHub CODEOWNERS file inside your editor",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-codeowners.open",
		"workspaceContains:**/CODEOWNERS"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-codeowners.open",
				"title": "Open CODEOWNERS"
			}
		],
		"languages": [
			{
					"id": "codeowners",
					"aliases": [
							"CODEOWNERS",
							"codeowners"
					],
					"filenames": [
							"CODEOWNERS"
					],
					"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
				{
						"language": "codeowners",
						"scopeName": "text.codeowners",
						"path": "./syntaxes/codeowners.tmLanguage.json"
				}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
